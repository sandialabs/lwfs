# Creates a 1 MB file full of zeros, then formats it as an EBOFS FS
@HAVE_EBOFS_TRUE@ebofs.disk: 
@HAVE_EBOFS_TRUE@	dd if=/dev/zero of=ebofs.disk bs=1k count=100000
@HAVE_EBOFS_TRUE@	$(MKFS_EBOFS) ebofs.disk
